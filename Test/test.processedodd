              <zeroOrMore>
                <ref name="model.global"/>
              </zeroOrMore>
            </oneOrMore>
            <zeroOrMore>
              <choice>
                <ref name="model.div2Like"/>
                <ref name="model.divGenLike"/>
              </choice>
              <zeroOrMore>
                <ref name="model.global"/>
              </zeroOrMore>
            </zeroOrMore>
          </group>
        </choice>
        <zeroOrMore>
          <group>
            <ref name="model.divBottom"/>
