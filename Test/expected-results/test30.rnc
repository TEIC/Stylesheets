             "\x{a}" ~
             "                  "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    empty
  }
TMenu =
  
  ##
  element ns2:Menu {
    text,
    Tatt.global.attribute.xmlid,
    Tatt.global.rendition.attribute.rend,
    empty
  }
