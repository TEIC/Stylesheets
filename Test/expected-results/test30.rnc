Ttext =
  
  ## contains a single text of any kind, whether unitary or composite, for example a poem or drama, a collection of essays, a novel, a dictionary, or a corpus sample. [4.  15.1. ]
  element text {
    (Tmodel.global*,
     (Tfront, Tmodel.global*)?,
     (Tbody | Tgroup),
     Tmodel.global*,
     (Tback, Tmodel.global*)?),
    Tatt.global.attributes,
    Tatt.declaring.attributes,
    Tatt.typed.attributes,
    empty
  }
Tbody =
  
  ## (text body) contains the whole body of a single unitary text, excluding any front or back matter. [4. ]
  element body {
    (Tmodel.global*,
     (Tmodel.divTop, (Tmodel.global | Tmodel.divTop)*)?,
     (Tmodel.divGenLike, (Tmodel.global | Tmodel.divGenLike)*)?,
     ((Tmodel.divLike, (Tmodel.global | Tmodel.divGenLike)*)+
      | (Tmodel.div1Like, (Tmodel.global | Tmodel.divGenLike)*)+
      | ((Tmodel.common, Tmodel.global*)+,
         ((Tmodel.divLike, (Tmodel.global | Tmodel.divGenLike)*)+
          | (Tmodel.div1Like,
             (Tmodel.global | Tmodel.divGenLike)*)+)?)),
     (Tmodel.divBottom, Tmodel.global*)*),
    Tatt.global.attributes,
    Tatt.declaring.attributes,
    empty
  }
Tgroup =
  
  ## contains the body of a composite text, grouping together a sequence of distinct texts (or groups of such texts) which are regarded as a unit for some purpose, for example the collected works of an author, a sequence of prose essays, etc. [4.  4.3.1.  15.1. ]
  element group {
    ((Tmodel.divTop | Tmodel.global)*,
     ((Ttext | Tgroup), (Ttext | Tgroup | Tmodel.global)*),
     Tmodel.divBottom*),
    Tatt.global.attributes,
    Tatt.declaring.attributes,
    Tatt.typed.attributes,
    empty
  }
TfloatingText =
  
  ## contains a single text of any kind, whether unitary or composite, which interrupts the text containing it at any point and after which the surrounding text resumes. [4.3.2. ]
  element floatingText {
    (Tmodel.global*,
     (Tfront, Tmodel.global*)?,
     (Tbody | Tgroup),
     Tmodel.global*,
     (Tback, Tmodel.global*)?),
    Tatt.global.attributes,
    Tatt.declaring.attributes,
    Tatt.typed.attributes,
    empty
  }
Tdiv =
  
  ## (text division) contains a subdivision of the front, body, or back of a text. [4.1. ]
  element div {
    ((Tmodel.divTop | Tmodel.global)*,
     ((((Tmodel.divLike | Tmodel.divGenLike), Tmodel.global*)+
       | ((Tmodel.common, Tmodel.global*)+,
          ((Tmodel.divLike | Tmodel.divGenLike), Tmodel.global*)*)),
      (Tmodel.divBottom, Tmodel.global*)*)?),
    Tatt.global.attributes,
    Tatt.divLike.attributes,
    Tatt.typed.attribute.subtype,
    Tatt.declaring.attributes,
    
    ## characterizes the element in some sense, using any convenient classification scheme or typology.
    attribute type {
      
      ##
      "Act"
      | 
        ##
        "Scene"
      | 
        ##
        "play"
      | 
        ##
        "letter"
    }?,
    empty
  }
Ttrailer =
  
  ## contains a closing title or footer appearing at the end of a division of a text. [4.2.4.  4.2. ]
  element trailer {
    (text
     | Tlg
     | Tmodel.gLike
     | Tmodel.phrase
     | Tmodel.inter
     | Tmodel.lLike
     | Tmodel.global)*,
    Tatt.global.attributes,
    Tatt.typed.attributes,
    empty
  }
Tbyline =
  
  ## contains the primary statement of responsibility given for a work on its title page or at the head or end of the work. [4.2.2.  4.5. ]
  element byline {
    (text | Tmodel.gLike | Tmodel.phrase | TdocAuthor | Tmodel.global)*,
    Tatt.global.attributes,
    empty
  }
Tdateline =
  
  ## contains a brief description of the place, date, time, etc. of production of a letter, newspaper story, or other work, prefixed or suffixed to it as a kind of heading or trailer. [4.2.2. ]
  element dateline {
    (text | Tmodel.gLike | Tmodel.phrase | Tmodel.global | TdocDate)*,
    Tatt.global.attributes,
    empty
  }
Targument =
  
  ## contains a formal list or prose description of the topics addressed by a subdivision of a text. [4.2.  4.6. ]
  element argument {
    ((Tmodel.global | Tmodel.headLike)*,
     (Tmodel.common, Tmodel.global*)+),
    Tatt.global.attributes,
    empty
  }
Tepigraph =
  
  ## contains a quotation, anonymous or attributed, appearing at the start or end of a section or on a title page. [4.2.3.  4.2.  4.6. ]
  element epigraph {
    (Tmodel.common | Tmodel.global)*, Tatt.global.attributes, empty
  }
Topener =
  
  ## groups together dateline, byline, salutation, and similar phrases appearing as a preliminary group at the start of a division, especially of a letter. [4.2. ]
  element opener {
    (text
     | Tmodel.gLike
     | Tmodel.phrase
     | Targument
     | Tbyline
     | Tdateline
     | Tepigraph
     | Tsalute
     | Tsigned
     | Tmodel.global)*,
    Tatt.global.attributes,
    empty
  }
Tcloser =
  
  ## groups together salutations, datelines, and similar phrases appearing as a final group at the end of a division, especially of a letter. [4.2.2.  4.2. ]
  element closer {
    (text
     | Tmodel.gLike
     | Tsigned
     | Tdateline
     | Tsalute
     | Tmodel.phrase
     | Tmodel.global)*,
    Tatt.global.attributes,
    empty
  }
Tsalute =
  
  ## (salutation) contains a salutation or greeting prefixed to a foreword, dedicatory epistle, or other division of a text, or the salutation in the closing of a letter, preface, etc. [4.2.2. ]
  element salute { Tmacro.paraContent, Tatt.global.attributes, empty }
Tsigned =
  
  ## (signature) contains the closing salutation, etc., appended to a foreword, dedicatory epistle, or other division of a text. [4.2.2. ]
  element signed { Tmacro.paraContent, Tatt.global.attributes, empty }
Tpostscript =
  
  ## contains a postscript, e.g. to a letter. [4.2. ]
  element postscript {
    ((Tmodel.global | Tmodel.divTopPart)*,
     Tmodel.common,
     (Tmodel.global | Tmodel.common)*,
     (Tmodel.divBottomPart, Tmodel.global*)*),
    Tatt.global.attributes,
    empty
  }
TtitlePage =
  
  ## (title page) contains the title page of a text, appearing within the front or back matter. [4.6. ]
  element titlePage {
    (Tmodel.global*,
     Tmodel.titlepagePart,
     (Tmodel.titlepagePart | Tmodel.global)*),
    Tatt.global.attributes,
    
    ## classifies the title page according to any convenient typology.
    attribute type { Tdata.enumerated }?,
    empty
  }
TdocTitle =
  
  ## (document title) contains the title of a document, including all its constituents, as given on a title page. [4.6. ]
  element docTitle {
    (Tmodel.global*, (TtitlePart, Tmodel.global*)+),
    Tatt.global.attributes,
    Tatt.canonical.attributes,
    empty
  }
TtitlePart =
  
  ## contains a subsection or division of the title of a work, as indicated on a title page. [4.6. ]
  element titlePart {
    Tmacro.paraContent,
    Tatt.global.attributes,
    
    ## specifies the role of this subdivision of the title.
    ## Suggested values include: 1] main; 2] sub; 3] alt; 4] short; 5] desc
    [ a:defaultValue = "main" ]
    attribute type {
      
      ## main title of the work
      "main"
      | 
        ## (subordinate) subtitle of the work
        "sub"
      | 
        ## (alternate) alternative title of the work
        "alt"
      | 
        ## abbreviated form of title
        "short"
      | 
        ## (descriptive) descriptive paraphrase of the work
        "desc"
      | xsd:Name
    }?,
    empty
  }
TdocAuthor =
  
  ## (document author) contains the name of the author of the document, as given on the title page (often but not always contained in a byline). [4.6. ]
  element docAuthor {
    Tmacro.phraseSeq,
    Tatt.global.attributes,
    Tatt.canonical.attributes,
    empty
  }
Timprimatur =
  
  ## contains a formal statement authorizing the publication of a work, sometimes required to appear on a title page or its verso. [4.6. ]
  element imprimatur {
    Tmacro.paraContent, Tatt.global.attributes, empty
  }
TdocEdition =
  
  ## (document edition) contains an edition statement as presented on a title page of a document. [4.6. ]
  element docEdition {
    Tmacro.paraContent, Tatt.global.attributes, empty
  }
TdocImprint =
  
  ## (document imprint) contains the imprint statement (place and date of publication, publisher name), as given (usually) at the foot of a title page. [4.6. ]
  element docImprint {
    (text
     | Tmodel.gLike
     | Tmodel.phrase
     | TpubPlace
     | TdocDate
     | Tpublisher
     | Tmodel.global)*,
    Tatt.global.attributes,
    empty
  }
TdocDate =
  
  ## (document date) contains the date of a document, as given on a title page or in a dateline. [4.6. ]
  element docDate {
    Tmacro.phraseSeq,
    Tatt.global.attributes,
    
    ## gives the value of the date in standard form, i.e. YYYY-MM-DD.
    attribute when { Tdata.temporal.w3c }?,
    empty
  }
Tfront =
  
  ## (front matter) contains any prefatory matter (headers, title page, prefaces, dedications, etc.) found at the start of a document, before the main body. [4.6.  4. ]
  element front {
    ((Tmodel.frontPart
      | Tmodel.pLike
      | Tmodel.pLike.front
      | Tmodel.global)*,
     (((Tmodel.div1Like,
        (Tmodel.div1Like | Tmodel.frontPart | Tmodel.global)*)
       | (Tmodel.divLike,
          (Tmodel.divLike | Tmodel.frontPart | Tmodel.global)*)),
      (Tmodel.divBottom, (Tmodel.divBottom | Tmodel.global)*)?)?),
    Tatt.global.attributes,
    Tatt.declaring.attributes,
    empty
  }
Tback =
  
  ## (back matter) contains any appendixes, etc. following the main part of a text. [4.7.  4. ]
  element back {
    ((Tmodel.frontPart
      | Tmodel.pLike.front
      | Tmodel.pLike
      | Tmodel.listLike
      | Tmodel.global)*,
     ((Tmodel.div1Like,
       (Tmodel.frontPart | Tmodel.div1Like | Tmodel.global)*)
      | (Tmodel.divLike,
         (Tmodel.frontPart | Tmodel.divLike | Tmodel.global)*))?,
     (Tmodel.divBottomPart, (Tmodel.divBottomPart | Tmodel.global)*)?),
    Tatt.global.attributes,
    Tatt.declaring.attributes,
    empty
  }
Tplatypus =
  
  ##
  element platypus {
    empty,
    (
     ##
     attribute bill { "duck" }?)
    >> sch:pattern [
         id = "platypus-constraint-silly-test"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:platypus/@bill"
           "\x{a}" ~
           "               "
           sch:report [
             test = "true()"
             "\x{a}" ~
             "		    My context is "
             sch:value-of [ select = "." ]
             "\x{a}" ~
             "		             "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    empty
  }
TMenu =
  
  ##
  element ns2:Menu {
    text, Tatt.global.attribute.xmlid, Tatt.global.attribute.rend, empty
  }
start = TTEI | Tdiv
