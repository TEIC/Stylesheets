Tptr =
  
  ## (pointer) defines a pointer to another location. [3.6.  16.1. ]
  element ptr {
    empty
    >> sch:pattern [
         id = "ptr-constraint-ptrAtts"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:ptr"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and @cRef"
             "Only one of the\x{a}" ~
             "attributes @target and @cRef may be supplied on "
             sch:name [ ]
             "."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    Tatt.global.attributes,
    Tatt.pointing.attributes,
    Tatt.internetMedia.attributes,
    Tatt.typed.attributes,
    Tatt.declaring.attributes,
    Tatt.cReferencing.attributes,
    empty
  }
Tref =
  
  ## (reference) defines a reference to another location, possibly modified by additional text or comment. [3.6.  16.1. ]
  element ref {
    Tmacro.paraContent
    >> sch:pattern [
         id = "ref-constraint-refAtts"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:ref"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and @cRef"
             "Only one of the\x{a}" ~
             "	attributes @target' and @cRef' may be supplied on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    Tatt.global.attributes,
    Tatt.pointing.attributes,
    Tatt.internetMedia.attributes,
    Tatt.typed.attributes,
    Tatt.declaring.attributes,
    Tatt.cReferencing.attributes,
    empty
  }
Tlist =
  
  ## (list) contains any sequence of items organized as a list. [3.7. ]
  element list {
    ((Tmodel.divTop | Tmodel.global)*,
     ((Titem, Tmodel.global*)+
      | (TheadLabel?,
         TheadItem?,
         (Tlabel, Tmodel.global*, Titem, Tmodel.global*)+)),
     (Tmodel.divBottom, Tmodel.global*)*),
    Tatt.global.attributes,
    Tatt.sortable.attributes,
    
    ## describes the form of the list.
    ## Suggested values include: 1] ordered; 2] bulleted; 3] simple; 4] gloss
    [ a:defaultValue = "simple" ]
    attribute type {
      
      ## list items are numbered or lettered.
      "ordered"
      | 
        ## list items are marked with a bullet or other typographic device.
        "bulleted"
      | 
        ## list items are not numbered or bulleted.
        "simple"
      | 
        ## each list item glosses some term or concept, which is given by a label element preceding the list item.
        "gloss"
      | xsd:Name
    }?,
    empty
  }
