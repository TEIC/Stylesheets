  <text>
    <body>
      <div>
        <div>
          <head>Simple ordered</head>
          <list rend="numbered">
            <item>Cats</item>
            <item>Bats</item>
            <item>Flats</item>
          </list>
          <p>Done</p>
        </div>
        <div>
          <head>Simple unordered</head>
          <list rend="bulleted">
            <item>Cats</item>
            <item>Bats</item>
            <item>Flats</item>
          </list>
          <p>Done</p>
        </div>
        <div>
          <head>Nested and ordered</head>
          <list rend="numbered">
            <item>Cats</item>
            <item>Dogs<list rend="numbered"><item>Alsatians</item><item>Greyhounds</item></list></item>
            <item>Sheep</item>
          </list>
          <p>done</p>
        </div>
        <div>
          <head>Nested and unordered</head>
          <list rend="bulleted">
            <item>Cats</item>
            <item>Dogs<list rend="bulleted"><item>Alsatians</item><item>Greyhounds</item></list></item>
            <item>sheep</item>
          </list>
          <p>done</p>
        </div>
        <div>
          <head>Now insert a List Number</head>
          <list rend="numbered">
            <item>Cats</item>
            <item>Bats</item>
            <item>Flats</item>
          </list>
        </div>
        <div>
          <head>List Number 2</head>
          <list rend="numbered">
            <item>Cats</item>
            <item>Bats</item>
            <item>Flats</item>
          </list>
        </div>
        <div>
          <head>List Bullet</head>
          <list rend="bulleted">
            <item>Cats</item>
            <item>Bats</item>
            <item>Flats</item>
          </list>
          <table rend="rules">
            <row>
              <cell style="text-align: left;">A</cell>
              <cell style="text-align: left;">B</cell>
              <cell style="text-align: left;">C</cell>
              <cell style="text-align: left;">D</cell>
              <cell style="text-align: left;">E</cell>
            </row>
            <row>
              <cell style="text-align: left;">1</cell>
              <cell style="text-align: left;">2</cell>
              <cell style="text-align: left;">3</cell>
              <cell style="text-align: left;">4</cell>
              <cell style="text-align: left;">5</cell>
            </row>
          </table>
        </div>
      </div>
    </body>
  </text>
