  <text>
    <body>
      <div>
        <div>
          <head>Simple ordered</head>
          <list type="ordered">
            <item>Cats</item>
            <item>Bats</item>
            <item>Flats</item>
          </list>
          <p>Done</p>
        </div>
        <div>
          <head>Simple unordered</head>
          <list type="unordered">
            <item>Cats</item>
            <item>Bats</item>
            <item>Flats</item>
          </list>
          <p>Done</p>
        </div>
        <div>
          <head>Nested and ordered</head>
          <list type="ordered">
            <item>Cats</item>
            <item>Dogs<list type="ordered"><item>Alsatians</item><item>Greyhounds</item></list></item>
            <item>Sheep</item>
          </list>
          <p>done</p>
        </div>
        <div>
          <head>Nested and unordered</head>
          <list type="unordered">
            <item>Cats</item>
            <item>Dogs<list type="unordered"><item>Alsatians</item><item>Greyhounds</item></list></item>
            <item>sheep</item>
          </list>
          <p>done</p>
        </div>
        <div>
          <head>Now insert a List Number</head>
          <list type="ordered">
            <item>Cats</item>
            <item>Bats</item>
            <item>Flats</item>
          </list>
        </div>
        <div>
          <head>List Number 2</head>
          <list type="ordered">
            <item>Cats</item>
            <item>Bats</item>
            <item>Flats</item>
          </list>
        </div>
        <div>
          <head>List Bullet</head>
          <list type="unordered">
            <item>Cats</item>
            <item>Bats</item>
            <item>Flats</item>
          </list>
          <table rend="rules">
            <row>
              <cell>A</cell>
              <cell>B</cell>
              <cell>C</cell>
              <cell>D</cell>
              <cell>E</cell>
            </row>
            <row>
              <cell>1</cell>
              <cell>2</cell>
              <cell>3</cell>
              <cell>4</cell>
              <cell>5</cell>
            </row>
          </table>
        </div>
      </div>
    </body>
  </text>
