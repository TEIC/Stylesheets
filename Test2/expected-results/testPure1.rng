            <text/>
            <ref name="t_model.limitedPhrase"/>
            <ref name="t_model.inter"/>
         </choice>
      </zeroOrMore>
   </define>
   <define name="t_macro.phraseSeq">
      <zeroOrMore>
         <choice>
            <text/>
            <ref name="t_model.gLike"/>
            <ref name="t_model.phrase"/>
            <ref name="t_model.global"/>
         </choice>
      </zeroOrMore>
   </define>
   <define name="t_macro.phraseSeq.limited">
      <zeroOrMore>
         <choice>
            <text/>
            <ref name="t_model.limitedPhrase"/>
            <ref name="t_model.global"/>
         </choice>
