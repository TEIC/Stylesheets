         <ref name="t_att.global.attributes"/>
         <ref name="t_att.typed.attributes"/>
         <ref name="t_att.edition.attributes"/>
         <ref name="t_att.spanning.attributes"/>
         <ref name="t_att.breaking.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="t_lb">
